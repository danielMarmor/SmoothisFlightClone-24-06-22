<script src='/jquery-3.6.3.js' type="text/javascript"></script>
<script type="text/javascript">
    window.addEventListener('load', (loadEvent)=>{
        const lnkImport = document.querySelector("#lnkImport");
        lnkImport.addEventListener('click', (clickEvent)=>{
            clickEvent.preventDefault();
            importUsers()
            .then(()=>{
                alert('Data Imported Succecfuly!');
            });
        });
    });
    const importUsers =()=>{
        const promise =new Promise((resolve, reject)=>{
            $.ajax({
                url: `http://localhost:3000/import_users`,
                type: 'post',
                dataType: 'json',
                statusCode: {
                    201: ()=> {
                        resolve();
                    }                       
                }
            });
        }); 
        return promise;        
    }
    

</script>​
